<wux-calendar id="wux-calendar" />

<view class="page">
  <view class="userinfo">
    <view class="userinfo-avatar">
      <open-data type="userAvatarUrl"></open-data>
    </view>
    <open-data type="userNickName"></open-data>
  </view>
  <view>
    <wux-cell-group>
      <wux-cell thumb="https://wux.cdn.cloverstd.com/logo.png" title="我的故事" extra="12 条" bindtap="linkto"></wux-cell>
      <wux-cell thumb="https://wux.cdn.cloverstd.com/logo.png" title="我的背书" extra="50 条"></wux-cell>
      <wux-cell thumb="https://wux.cdn.cloverstd.com/logo.png" title="为我背书" extra="20 人"></wux-cell>
    </wux-cell-group>
  </view>
</view>